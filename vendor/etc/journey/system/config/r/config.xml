    <package label="email"
        behavior="ALLOW_PERSIST,"
        names="me.bluemail.mail,com.microsoft.office.outlook,com.tohsoft.mail.email.emailclient,"
        area="WIKO_FR,"
    />
    
    <!-- google app -->
    <package label="google app"
        behavior="ALLOW_PERSIST,"
        names="com.google.android.apps.cloudprint,"
    />
        
    <!-- myos -->
    <package label="myos"
        behavior="ASSUMING_IN_SYSTEM,"
        names="com.sugar.note,com.ape.weather3,com.ape.weatherlive,com.ape.apetrialfeedback,com.ape.weathergo,com.ape.qhb,"
    />
    
    <package label="speechassist"
