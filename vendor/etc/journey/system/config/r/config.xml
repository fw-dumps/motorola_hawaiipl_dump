    />
    
    <package label="appsstore"
        behavior="ASSUMING_IN_SYSTEM,"
        names="com.myos.appsstore,com.ape.ufogames,"
    />

    <package label="myseneschal"
        behavior="ASSUMING_IN_SYSTEM,"
        names="com.ape.myseneschal,com.myos.myseneschal,com.ape.smartassist,com.ape.cleanassist,"
    />

    <!-- system app not in system for cta -->
    <package label="cta"
        behavior="ASSUMING_IN_SYSTEM,"
        names="com.android.email.provider,com.android.email,com.android.soundrecorder,"
    />
    
    <!-- customer request -->
	<package label="customer for HK"
        behavior="ASSUMING_IN_SYSTEM,"
        names="com.smartone.aon,com.smartone.wificalling," 
        area="SUGAR_HK,SUGAR_TW,"
    />

	<package label="customer for Sugar"
        behavior="ASSUMING_IN_SYSTEM,"
        names="com.redteamobile.global.roaming," 
        area="SUGAR_CN,SUGAR_HK,SUGAR_TW,"
    />    
    

    <!-- ALLOW_LOCATION -->
    <package label="baidu map"
        behavior="ALLOW_LOCATION,"
        names="com.baidu.BaiduMap,"
    />

    <package label="amap"
        behavior="ALLOW_LOCATION,"
        names="com.amap.android,com.autonavi.minimap,"
    />

    <package label="didi"
        behavior="ALLOW_LOCATION,"
        names="com.sdu.didi.gsui,com.funcity.taxi.driver,"
    />

	<!-- Sports positioning -->
    <package label="Sports positioning"
        behavior="ALLOW_LOCATION,"
        names="im.xingzhe,com.bamboo.ibike,com.topgether.sixfoot,cc.iriding.mobile,com.ape.pedometer,com.codoon.gps,com.yuedong.sport,"
    />

    <!-- disabled component begin -->
    <package label="igexin_component"
        behavior="DISABLED_COMPONENT,"
        names="com.nexstreaming.app.assetstore.tinno.china/com.igexin.sdk.PushReceiver,com.nexstreaming.app.assetstore.tinno.china/com.igexin.sdk.PushService,"
        area="SUGAR_CN,"
    />

    <!-- disabled component end -->


    <!-- reject bg caller call package begin-->
    <package label="qqdownloader"
        behavior="ASSUMING_NOT_IN_SYSTEM,"
        names="com.tencent.android.qqdownloader,"
    />
    <!-- reject bg caller call package end-->

    <!-- reject activity begin-->
    <package label="webo_activities_reject"
        behavior="REJECT_ACTIVITY_WAKEUP,"
        names="com.sina.weibo.page.NewCardListActivity,"
    />

    <!-- 3rd push server activity begin-->
    <package label="jpush_activities_reject"
        behavior="REJECT_ACTIVITY_WAKEUP,"
        names="cn.jpush.android.ui.PushActivity,cn.jpush.android.ui.PopWinActivity,"
    />

    <package label="igexin_activities_reject"
        behavior="REJECT_ACTIVITY_WAKEUP,"
        names="com.igexin.sdk.PushActivity,com.igexin.sdk.GActivity,com.igexin.getuiext.activity.GetuiExtActivity,"
    />
    <!-- 3rd push server activity end-->

    <!-- reject activity end-->

    <package label="lab_test_mode"
        behavior="NO_LIMIT,"
        names="com.orangelabs*,org.opencv.engine,com.android.cts.verifier,com.tinno.*,com.lunarlabs.panda,com.smartviser.visersettings,"
    />

    <!-- The black caller begin-->
    <package label="black caller"
        behavior="DISALLOW_BLACK_CALLER,"
        names="com.tencent.android.qqdownloader,"
    />
    <!--  The black caller end-->

</config>
